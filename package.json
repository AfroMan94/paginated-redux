{
  "name": "paginated-redux",
  "version": "0.1.2",
  "description": "A higher order reducer (or transducer) that adds pagination, sorting, and filtering to a reducer of an array of objects.",
  "main": "dist/paginated-redux.js",
  "scripts": {
    "build": "node_modules/babel-cli/bin/babel.js ./src --out-dir ./dist",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robmclarty/paginated-redux.git"
  },
  "keywords": [
    "redux",
    "reducer",
    "transducer",
    "pagination",
    "sort",
    "filter",
    "page",
    "array"
  ],
  "author": "Rob McLarty <r@robmclarty.com> (robmclarty.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robmclarty/paginated-redux/issues"
  },
  "homepage": "https://github.com/robmclarty/paginated-redux#readme",
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.6.0"
  }
}
